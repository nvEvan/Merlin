<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration SYSTEM "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
 <hibernate-configuration> 
 	<session-factory>
 		<!-- Config database -->
 		<property name="hibernate.connectino.driver_class">
 			oracle.jdbc.OracleDriver
 		</property>
 		
 		<property name="hibernate.dialect">
 			org.hibernate.dialect.Oracle10gDialect
 		</property>
 		
 		<!--  CLASS MAPPING  -->
 		<mapping class="com.revature.merlinserver.beans.MagicalUser" />
 		<mapping class="com.revature.merlinserver.beans.CodeList" />
 		<mapping class="com.revature.merlinserver.beans.PrivateUserInfo" />
 		<mapping class="com.revature.merlinserver.beans.Appointment" />
 		<mapping class="com.revature.merlinserver.beans.SessionAvailability" />
 		<mapping class="com.revature.merlinserver.beans.Receipt" />
 		<mapping class="com.revature.merlinserver.beans.PublicUserInfo" />
 		<mapping class="com.revature.merlinserver.beans.AdeptInfo" />
 		<mapping class="com.revature.merlinserver.beans.AdeptSkill" />
 		<mapping class="com.revature.merlinserver.beans.Badge" />
 		<mapping class="com.revature.merlinserver.beans.Review" />
 		<mapping class="com.revature.merlinserver.beans.UserBadge" />
 		<mapping class="com.revature.merlinserver.beans.FileUpload" />
 	</session-factory>
 </hibernate-configuration> 